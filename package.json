{
  "name": "@rethinkhealth/hl7v2-workspace",
  "description": "Modern Typescript HL7v2 workspace",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Melek Somai",
    "email": "melek@rethinkhealth.io"
  },
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm --filter=@rethinkhealth/* run build",
    "test": "pnpm --filter=@rethinkhealth/* run test",
    "clean": "rimraf packages/*/dist packages/*/node_modules node_modules",
    "ci:install": "pnpm install --no-frozen-lockfile",
    "ci:publish": "pnpm build && pnpm recursive publish --no-git-checks",
    "ci:update": "pnpm --recursive update",
    "ci:version": "pnpm changeset version",
    "syncpack": "npx syncpack",
    "syncpack:fix": "npx syncpack fix-mismatches && npx syncpack set-semver-ranges && npx syncpack format"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.4",
    "only-allow": "^1.1.1"
  },
  "repository": "rethinkhealth/hl7v2.git",
  "homepage": "https://www.rethinkhealth.io/hl7v2/docs",
  "keywords": ["health", "healthcare", "hl7", "hl7v2", "nodejs", "typescript"],
  "packageManager": "pnpm@10.12.1",
  "publishConfig": {
    "access": "public"
  }
}
