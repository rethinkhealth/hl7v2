{
  "name": "@rethinkhealth/hl7v2",
  "description": "HL7v2 client for NodeJS.",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Melek Somai",
  "keywords": [],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "benchmarks": "ts-node benchmarks/benchmarks.ts",
    "build": "tsup && pnpm copy",
    "copy":  "mkdir -p 'dist/schema' && cp -R 'src/schema'  'dist'",
    "ci:install": "pnpm install --no-frozen-lockfile",
    "ci:publish": "pnpm build && pnpm recursive publish",
    "ci:update": "pnpm --recursive update",
    "ci:version": "pnpm changeset version",
    "clean": "rm -rf node_modules && rm -rf dist",
    "cli": "ts-node ./cli/index.ts",
    "dev": "tsup --watch",
    "lint": "TIMING=1 eslint 'src/**/*.{js,ts}'",
    "lint:fix": "TIMING=1 eslint 'src/**/*.{js,ts}' --fix",
    "syncpack": "npx syncpack",
    "syncpack:fix": "npx syncpack fix-mismatches && npx syncpack set-semver-ranges && npx syncpack format",
    "test": "jest --coverage --passWithNoTests",
    "test:update": "jest -u"
  },
  "peerDependencies": {
    "zod": "3.x.x"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.0",
    "@commander-js/extra-typings": "10.0.2",
    "@types/benchmark": "2.1.2",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.14.1",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "ajv": "8.12.0",
    "benchmark": "2.1.4",
    "better-ajv-errors": "1.2.0",
    "commander": "10.0.0",
    "esbuild": "0.17.10",
    "esbuild-jest": "0.5.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-yaml": "0.5.0",
    "fast-xml-parser": "4.1.3",
    "jest": "29.4.3",
    "jsonata": "2.0.2",
    "prettier": "2.8.4",
    "syncpack": "9.8.4",
    "tiny-glob": "^0.2.9",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsup": "6.6.3",
    "typed-emitter": "2.1.0",
    "typescript": "4.9.5",
    "zod": "3.20.6"
  }
}
