{
  "name": "@rethinkhealth/hl7v2",
  "description": "HL7v2 client for NodeJS.",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Melek Somai",
  "keywords": [],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf node_modules && rm -rf dist",
    "cli": "ts-node ./src/cli.ts",
    "dev": "tsup --watch",
    "lint": "TIMING=1 eslint 'src/**/*.{js,ts}'",
    "lint:fix": "TIMING=1 eslint 'src/**/*.{js,ts}' --fix",
    "syncpack": "npx syncpack",
    "syncpack:fix": "npx syncpack fix-mismatches && npx syncpack set-semver-ranges && npx syncpack format",
    "test": "jest --coverage --passWithNoTests",
    "test:update": "jest -u",
    "ci:install": "pnpm install --no-frozen-lockfile",
    "ci:publish": "pnpm build && pnpm recursive publish",
    "ci:version": "pnpm changeset version",
    "ci:update": "pnpm --recursive update",
    "benchmarks": "ts-node benchmarks/benchmarks.ts"
  },
  "peerDependencies": {
    "zod": "3.x.x"
  },
  "dependencies": {
    "jsonata": "2.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.0",
    "@commander-js/extra-typings": "^10.0.2",
    "@types/benchmark": "^2.1.2",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "benchmark": "^2.1.4",
    "commander": "^10.0.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-yaml": "0.5.0",
    "fast-xml-parser": "^4.1.2",
    "figlet": "^1.5.2",
    "jest": "29.3.1",
    "prettier": "2.8.4",
    "syncpack": "8.4.11",
    "ts-jest": "29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "6.6.3",
    "typescript": "4.9.5",
    "zod": "3.20.6"
  }
}
