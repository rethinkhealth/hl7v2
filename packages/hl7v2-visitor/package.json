{
  "name": "@rethinkhealth/hl7v2-visitor",
  "description": "An AST visitor for HL7v2 messages",
  "version": "0.3.2",
  "license": "MIT",
  "author": {
    "name": "Melek Somai",
    "email": "melek@rethinkhealth.io"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "peerDependencies": {
    "unified": ">=10"
  },
  "devDependencies": {
    "@rethinkhealth/hl7v2-ast": "workspace:*",
    "@rethinkhealth/hl7v2-builder": "workspace:*",
    "@rethinkhealth/testing": "workspace:*",
    "@rethinkhealth/tsconfig": "workspace:*",
    "@types/node": "24.10.0",
    "@vitest/coverage-v8": "^4.0.5",
    "tsup": "8.5.0",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vitest": "^4.0.6"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "sideEffects": false
}
