{
  "name": "@rethinkhealth/hl7v2-preset-lint-recommended",
  "description": "hl7v2-lint preset with recommended rules",
  "version": "0.2.13",
  "license": "MIT",
  "author": {
    "name": "Melek Somai",
    "email": "melek@rethinkhealth.io"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@rethinkhealth/hl7v2-lint-no-trailing-empty-field": "workspace:*",
    "@rethinkhealth/hl7v2-lint-required-message-header": "workspace:*",
    "@rethinkhealth/hl7v2-lint-segment-header-length": "workspace:*",
    "unified": "11.0.5",
    "unified-lint-rule": "3.0.1"
  },
  "devDependencies": {
    "@rethinkhealth/hl7v2-ast": "workspace:*",
    "@rethinkhealth/hl7v2-jsonify": "workspace:*",
    "@rethinkhealth/hl7v2-parser": "workspace:*",
    "@rethinkhealth/testing": "workspace:*",
    "@rethinkhealth/tsconfig": "workspace:*",
    "@types/node": "24.5.2",
    "@types/unist": "^3.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "8.5.0",
    "typescript": "^5.9.2",
    "vfile-reporter": "^8.1.1",
    "vitest": "^3.2.4"
  },
  "repository": "rethinkhealth/hl7v2.git",
  "homepage": "https://www.rethinkhealth.io/hl7v2/docs",
  "keywords": [
    "definition",
    "hl7",
    "hl7v2",
    "hl7v2-lint",
    "hl7v2-lint-rule",
    "lint",
    "nodejs",
    "rule",
    "typescript"
  ],
  "packageManager": "pnpm@10.14.0",
  "publishConfig": {
    "access": "public"
  }
}
