{
  "name": "@rethinkhealth/hl7v2-annotator-message",
  "description": "Unified plugin that annotates HL7v2 root with version and message type metadata from MSH-9/12.",
  "version": "0.0.4",
  "license": "MIT",
  "author": {
    "name": "Melek Somai",
    "email": "melek@rethinkhealth.io"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "unified": ">=10"
  },
  "dependencies": {
    "@rethinkhealth/hl7v2-util-query": "workspace:*",
    "@rethinkhealth/hl7v2-utils": "workspace:*",
    "unist-util-visit": "5.0.0"
  },
  "devDependencies": {
    "@rethinkhealth/hl7v2-ast": "workspace:*",
    "@rethinkhealth/hl7v2-builder": "workspace:*",
    "@rethinkhealth/testing": "workspace:*",
    "@rethinkhealth/tsconfig": "workspace:*",
    "@types/node": "24.8.1",
    "@types/unist": "^3.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "8.5.0",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "vfile": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
